{
  "mcpServers": {
    "focus": {
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/path/to/focus-mcp",
        "python",
        "/path/to/focus-mcp/focus_mcp_server.py"
      ],
      "env": {
        "FOCUS_DATA_PATH": "/path/to/your/focus/data"
      }
    }
  }
}